{
   "autoScalingGroupProvider": {
      "autoScalingGroupArn": "arn:aws:autoscaling:{Region}:{Account}:autoScalingGroup:{Uuid}:autoScalingGroupName/e3s-{Env}-asg",
      "managedScaling": {
         "status": "ENABLED",
         "targetCapacity": 100,
         "minimumScalingStepSize": 1,
         "maximumScalingStepSize": 100,
         "instanceWarmupPeriod": 300
      },
      "managedTerminationProtection": "DISABLED"
   }
}
